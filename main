from tkinter import *

master = Tk()

canvas_width = 802
canvas_height = 402
w = Canvas(master,
           width=canvas_width,
           height=canvas_height)
w.pack()

y = int(canvas_height / 2)
w.create_line(0, y, canvas_width, y, fill="#476042")
x = int(canvas_width / 2)
w.create_line(x, 0, canvas_height, x, fill="#476042")

mainloop()
